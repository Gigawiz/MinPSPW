+	}
+	return ((float)PSP_MATH_PI / 4) + _pspFpuAtanMain((x - 1.0f) / (x + 1.0f));
+}
+
+
+float pspFpuAtan(float x)
+{
+	int sign = 1;
+
+	/* NaN */
+	if (x != x) {
+		return x;
+	}
+
+	if (x < 0.0f) {
+		x = -x;
+		sign = -1;
+	}
+	if (x >= 1.0f) {
+		/*J atan(x) = pi/2 - atan(1/x) */
+		x = 1.0f / x;
+		x = ((float)(PSP_MATH_PI / 2)) - _atanf(x);
+	} else {
