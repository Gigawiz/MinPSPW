+	 *  =2^z * (e^(f/2))^2
+	 *  =2^z * (e^(f/4))^4
+	 * where y=x/log 2
+	 * z=nearest(y) , -0.5 <= f=y-z < 0.5
+	 */
+	union {
+		float f;
+		int i;
+	} fi;
