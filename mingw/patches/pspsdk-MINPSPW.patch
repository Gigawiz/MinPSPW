+{
+	x = x*x;
+	return pspFpuAtan(__builtin_allegrex_sqrt_s(x/(1.0f-x)));
+}
+
+float pspFpuAcos(float x)
+{
+	x = x*x;
+	return pspFpuAtan(__builtin_allegrex_sqrt_s((1.0f-x)/x));
